{
    "headers": [
      {
        "source": "/(.*)\\.js$",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ],
    "rewrites": [
      { 
        "source": "/api/:path*",
        "destination": "https://danjitalk.duckdns.org/api/:path*"
      },
      { 
        "source": "/danji-talk-frontend/:path*",
        "destination": "/:path*"
      },
      { 
        "source": "/(.*)", 
        "destination": "/index.html" 
      }
    ]
  } 