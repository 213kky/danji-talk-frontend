{
  "categories": [
    {
      "title": "## 🔄 주요 변경 사항",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 버그 수정",
      "labels": ["bug", "bugfix", "fix"]
    },
    {
      "title": "## 🔒 보안 업데이트",
      "labels": ["security", "dependencies"]
    },
    {
      "title": "## 📚 문서",
      "labels": ["documentation"]
    },
    {
      "title": "## 🧪 테스트",
      "labels": ["test"]
    },
    {
      "title": "## 🔧 기타 변경 사항",
      "labels": []
    }
  ],
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "변경 사항이 없습니다",
  "label_extractor": [
    {
      "pattern": "^(fix|fixes|bugfix|bug)(\\(.*\\))?:\\s?(.*)$",
      "target": "bug"
    },
    {
      "pattern": "^(feat|feature)(\\(.*\\))?:\\s?(.*)$",
      "target": "feature"
    },
    {
      "pattern": "^(doc|docs|document|documentation)(\\(.*\\))?:\\s?(.*)$",
      "target": "documentation"
    },
    {
      "pattern": "^(security)(\\(.*\\))?:\\s?(.*)$",
      "target": "security"
    },
    {
      "pattern": "^(test|tests)(\\(.*\\))?:\\s?(.*)$",
      "target": "test"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "tag_resolver": {
    "method": "semver"
  },
  "base_branches": [
    "master"
  ]
} 